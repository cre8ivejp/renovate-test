{
  "name": "renovate-test",
  "version": "1.0.0",
  "description": "Testing Renovate configuration with javascript-client-sdk dependencies",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "unbuild --sourcemap",
    "test": "pnpm test:browser --run ; pnpm test:node --run",
    "test:browser": "vitest --config ./vitest-browser.config.ts --dir test",
    "test:node": "vitest --config ./vitest-node.config.ts --dir test",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@types/jsdom": "21.0.0",
    "@types/node": "20.0.0",
    "@typescript-eslint/eslint-plugin": "7.0.0",
    "@typescript-eslint/parser": "7.0.0",
    "@vitest/browser": "2.0.0",
    "@vitest/utils": "2.0.0",
    "eslint": "9.0.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.0",
    "happy-dom": "14.0.0",
    "jiti": "1.21.0",
    "msw": "2.0.0",
    "prettier": "3.0.0",
    "typescript": "5.3.0",
    "typescript-eslint": "7.0.0",
    "unbuild": "2.0.0",
    "vitest": "2.0.0",
    "webdriverio": "8.0.0"
  },
  "packageManager": "pnpm@9.15.9",
  "dependencies": {
    "async-mutex": "0.4.0"
  }
}

